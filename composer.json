{
    "name": "eonx-com/easy-async",
    "description": "Makes dealing with async processes easier",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "eonx-com/easy-event-dispatcher": "^2.6",
        "eonx-com/easy-pagination": "^2.6",
        "eonx-com/easy-random": "^2.6",
        "nette/utils": "^3.1"
    },
    "require-dev": {
        "ramsey/uuid": "^3.9",
        "doctrine/dbal": "^2.6",
        "phpunit/phpunit": "^7.5 || ^8.4",
        "symfony/symfony": "^4.2 || ^5.0",
        "mockery/mockery": "^1.2",
        "laravel/lumen-framework": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyAsync\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyAsync\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.6-dev"
        }
    }
}
